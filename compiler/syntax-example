box f : 1 -> 3
link . f




(*
STAGE 1:
    f : i1
        o1, o2, o3

    g : i2, i3, i4
        o4

STAGE 2:
    y -> b
    z -> c
    x -> a

STAGE 3:
    f : i1
        x, y, z
    g : a, b, c
        o4
*)

(*
          -----[g]-----
          |
---[f]----
          |
          -----[h]-----


box f : 1 -> 2.
box g : 1 -> 1.
box h : 1 -> 1;
link x a, y b.
out x y. f | in a b. (g + h)
*)
